
@model List<myproject.Models.Time>

 @{
                     Layout = "~/Views/Shared/_Layoutadmin.cshtml";
  }

<header class="main">
                        <br />
					        <h2> View Schedule </h2>
					</header>

<table>
    <tr>
       <th>
            tname
        </th>
        <th>
            stn_name
        </th>
        <th>
            Ariv_time
        </th>
        <th>
            Depart_time
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @item.tname
        </td>
        <td>
            @item.stn_name
        </td>
        <td>
           @item.Ariv_time
        </td>
        <td>
           @item.Depart_time
        </td>
       
        <td>
            @Html.ActionLink(".","Updateschedule",  new { id=item.Id}, new { @class="icon fa fa-pencil fa-fw"})  |
            @Html.ActionLink( ".","Delschedule", new { id=item.Id }, new { @class="icon fa fa-trash-o fa-fw"}) 
        </td>
    </tr>
}
</table>
